URxvt.geometry: 86x24
URxvt.font: xft:ProggyCleanTTSZBP:style=Regular:pixelsize=30

Xft.dpi:        120
Xft.antialias:  false
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

URxvt*inheritPixmap: true
URxvt.internalBorder: 0
URxvt.jumpScroll: true
URxvt.loginShell: true
URxvt.perl-ext-common: default,matcher,searchable-scrollback,selection-to-clipboard
URxvt.pointerBlank: true
URxvt.saveLines: 4000
URxvt.secondaryScroll: true
URxvt.scrollBar: false
URxvt.scrollTtyKeypress: true
URxvt.scrollWithBuffer: true
URxvt*urlLauncher: /usr/bin/lynx
URxvt*background: #000000
URxvt*foreground: white
URxvt*cursorColor: white
URxvt*colorUL: white
URxvt*underlineColor: white
URxvt*color0: #000000
URxvt*color1: #9e1828
URxvt*color2: #aece92
URxvt*color3: #968a38
URxvt*color4: #414171
URxvt*color5: #963c59
URxvt*color6: #418179
URxvt*color7: #bebebe
URxvt*color8: #666666
URxvt*color9: #cf6171
URxvt*color10: #c5f779
URxvt*color11: #fff796
URxvt*color12: #4186be
URxvt*color13: #cf9ebe
URxvt*color14: #71bebe
